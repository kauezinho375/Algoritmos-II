#include <stdio.h>
#include <stdlib.h>
    int determinante3x3(int m[3][3])
    {
        return m[0][0]*(m[1][1]*m[2][2] - m[1][2]*m[2][1])
                -m[0][1]*(m[1][0]*m[2][2] - m[1][2]*m[2][0])
                +m[0][2]*(m[1][0]*m[2][1] - m[1][1]*m[2][0]);
    }
int main()
{
    int matriz[3][3] =
    {
        {2, 5, 3},
        {1, -2, -1},
        {3, 6, 4}
    };

    printf("Determinante da matriz: %d\n", determinante3x3(matriz));
}
