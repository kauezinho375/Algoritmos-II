#include <stdio.h>

int main()
{
    int matriz[3][3]; // Declaração de uma matriz 3x3
    int i, j; // i = linha | j = Coluna.
    
    // Preenchendo a matriz com valores de 1 a 9
    for(i = 0; i < 3; i++)
    {
        for(j = 0; j < 3; j++)
        {
        matriz[i][j] = i*3 + j + 1; // atribui valores sequenciais
        }
    }
    // imprimindo a matriz (usa a mesma logica que a de cima mas coloca um printf)
    for(i = 0; i < 3; i++)
    {
        for(j = 0; j < 3; j++)
        {
            printf("%d ", matriz[i][j]); // mostra o valor da matiz
        }
        printf("\n"); // pula linha da matriz
    }
    
    return 0;
}
