#include <stdio.h>

int main()
{
    int matriz [2][3] = {{1,2,3},{4,5,6}}; // Declaração
    int i, j, pares = 0;
    
    for(i = 0; i < 2; i++)
    {
        for(j = 0; j < 3; j++)
        {
            if(matriz[i][j] % 2 == 0) // verifica se elemento é par
            {
                pares++; // incrementa contador de pares
            }
        }
    }

    for(i = 0; i < 1; i++)
    {
        for(j = 0; j < 1; j++)
        {
            printf("Quantidades de Elementos pares: %d\n ", pares);
        }
    }
    return 0;
}
